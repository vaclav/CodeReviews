<idea-plugin version="2" xmlns:xi="http://www.w3.org/2001/XInclude">
  <id>CodeReviews</id>
  <name>CodeReviews</name>
  <category>Custom Languages</category>
  <description>MPS code review support</description>
  <version>2019.2</version>
  <vendor url="https://github.com/vaclav/CodeReviews" logo="/MPS_16.png">Vaclav Pech</vendor>
  <idea-version since-build="192.1" until-build="193.1"/>

  <depends>jetbrains.mps.core</depends>

  <extensions defaultExtensionNs="com.intellij">
    <mps.LanguageLibrary dir="/"/>
    <errorHandler implementation="jetbrains.mps.ide.blame.CharismaReporter"/>
  </extensions>
</idea-plugin>
